<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		<template id="kdvn_website_theme_css" name="Kdvn Theme Css" inherit_id="kdvn_website.kdvn_css">
			<xpath expr="//link[@href='/kdvn_website/static/src/css/kdvn_website.css']" position="after">
				<link href="/kdvn_website_theme/static/src/css/kdvn_website_theme.css" rel="stylesheet"/>
			</xpath>
		</template>
		<template id="kdvn_website_theme_js" name="Kdvn Theme Js" inherit_id="website.assets_frontend">
      		<xpath expr="script[last()]" position="after">
        		<script type="text/javascript" src="/kdvn_website_theme/static/src/js/kdvn_website_theme.js" />
      		</xpath>
    	</template>
	</data>
</openerp>
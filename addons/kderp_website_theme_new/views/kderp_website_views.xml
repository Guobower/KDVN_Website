<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="kderp_website_menu_display" model="ir.ui.view">
            <field name="name">Website.menu: Add icon in menu website</field>
            <field name="model">website.menu</field>
            <field name="inherit_id" ref="website.menu_tree"/>
            <field name="arch" type="xml">
                <field name="url" position="after">
                    <field name="fa_icon"/>
                </field>
            </field>
        </record>
		
		<record id="kderp_website_res_partner" model="ir.ui.view">
            <field name="name">res.partner: Add position in contact us</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <field name="website" position="before">
                    <field name="position_x"/>
                    <field name="position_y"/>
                </field>
            </field>
        </record>
		
    </data>
</openerp>

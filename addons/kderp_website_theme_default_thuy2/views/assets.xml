<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		<template id="show_post_style_thuy2" name="Show Post Style" inherit_id="kderp_website_theme_default.theme_style">
		    <xpath expr="link[last()]" position="after">
				<link href="/kderp_website_theme_default_thuy2/static/src/css/kdvn_website_thuy2.css" rel="stylesheet"/>
		    </xpath>
		</template>
		<template id="extend_show_post_style_thuy2" name="Extend Show Post Style" inherit_id="kderp_event.kderp_event_style">
			<xpath expr="//link[@href='/kderp_event/static/src/css/style.css']" position="after">
				<link href="/kderp_website_theme_default_thuy2/static/src/css/kdvn_website_thuy2.css" rel="stylesheet"/>
				<!--Cho Slice Project-->
				<link href="http://www.responsivejqueryslider.com/allinone_carousel.css" rel="stylesheet" type="text/css"/>
				<link href='http://fonts.googleapis.com/css?family=Droid+Sans' rel='stylesheet' type='text/css'/>
				<link href='http://fonts.googleapis.com/css?family=Cabin' rel='stylesheet' type='text/css'/>
		
		
			</xpath>
		</template>
		<template id="thuy2_theme_js" name="Thuy2 Theme JS" inherit_id="website.assets_frontend">
      		<xpath expr="script[last()]" position="after">
      			<!--Them duong dan thu vien js cho Slice trong Project-->
				<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script>
				<script src="http://www.responsivejqueryslider.com/js/jquery.touchSwipe.min.js" type="text/javascript"></script>
				<script src="http://www.responsivejqueryslider.com/js/allinone_carousel.js" type="text/javascript"></script>
				<!---->
        		<script type="text/javascript" src="/kderp_website_theme_default_thuy2/static/src/js/theme.js" />
      		</xpath>
    	</template>
	</data>
</openerp>

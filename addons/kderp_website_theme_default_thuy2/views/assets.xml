<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		<template id="show_post_style_thuy2" name="Show Post Style" inherit_id="kderp_website_theme_default.theme_style">
		    <xpath expr="link[last()]" position="after">
				<link href="/kderp_website_theme_default_thuy2/static/src/css/kdvn_website_thuy2.css" rel="stylesheet"/>
		    </xpath>
		</template>
		<template id="extend_show_post_style_thuy2" name="Extend Show Post Style" inherit_id="kderp_event.kderp_event_style">
			<xpath expr="//link[@href='/kderp_event/static/src/css/style.css']" position="after">
				<link href="/kderp_website_theme_default_thuy2/static/src/css/kdvn_website_thuy2.css" rel="stylesheet"/>
			</xpath>
		</template>
		<template id="thuy2_theme_js" name="Thuy2 Theme JS" inherit_id="website.assets_frontend">
      		<xpath expr="script[last()]" position="after">
        		<script type="text/javascript" src="/kderp_website_theme_default_thuy2/static/src/js/theme.js" />
      		</xpath>
    	</template>
	</data>
</openerp>

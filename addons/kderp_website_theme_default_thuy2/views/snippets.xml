        <div>
          <div class="oe_snippet_thumbnail">
            <img class="oe_snippet_thumbnail_img" src="http://placehold.it/100x79.png?text=News" />
            <span class="oe_snippet_thumbnail_title">Jumbo Certificates</span>
          </div>
          <section class="oe_snippet_body">
            <div class="container">
              <div class="jumbotron text-center" style="background:url(/website/image/ir.attachment/2585_59a80ba/datas) center center; background-size:cover;min-height: 231px;">
                <h1 style="font-size: 48px;"><span class="text-success">Certificates</span></h1>
              </div>
            </div>
          </section>
        </div>
      </xpath>
    </template>
</data>
</openerp>

<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		<template id="show_post_style_thuy2" name="Show Post Style" inherit_id="website.assets_frontend">
		    <xpath expr="link[last()]" position="after">
				<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css"/>
				<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css"/>
				<link href="/kderp_website_theme_default_thuy2/static/src/css/kdvn_website_thuy2.css" rel="stylesheet"/>
		    </xpath>
		</template>

		<template id="thuy2_theme_js" name="Thuy2 Theme JS" inherit_id="website.assets_frontend">
      		<xpath expr="script[last()]" position="after">
        		<script type="text/javascript" src="/kderp_website_theme_default_thuy2/static/src/js/theme.js" />
      		</xpath>
    	</template>
	</data>
</openerp>
